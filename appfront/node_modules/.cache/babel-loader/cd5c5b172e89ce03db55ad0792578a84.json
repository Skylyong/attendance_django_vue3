{"ast":null,"code":"import { defineComponent, reactive, computed } from 'vue';\nimport { UserOutlined, LockOutlined } from '@ant-design/icons-vue';\nimport router from '@/router';\nimport { postSubmit } from '../api/api.js';\nexport default defineComponent({\n  components: {\n    UserOutlined,\n    LockOutlined\n  },\n\n  setup() {\n    const formState = reactive({\n      username: '',\n      password: '',\n      remember: true\n    });\n\n    const onFinish = values => {\n      postSubmit(values.username, values.password).then(response => {\n        // this.message = ''\n        router.push({\n          path: '/'\n        });\n      }, response => {// this.message = 'username or passwords error!'\n      });\n    };\n\n    const onFinishFailed = errorInfo => {\n      console.log('Failed:', errorInfo);\n    };\n\n    const disabled = computed(() => {\n      return !(formState.username && formState.password);\n    });\n    return {\n      formState,\n      onFinish,\n      onFinishFailed,\n      disabled\n    };\n  }\n\n});","map":{"version":3,"mappings":"AAmDA,SAASA,eAAT,EAA0BC,QAA1B,EAAoCC,QAApC,QAAoD,KAApD;AACA,SAASC,YAAT,EAAuBC,YAAvB,QAA2C,uBAA3C;AACA,OAAOC,MAAP,MAAmB,UAAnB;AACA,SAAQC,UAAR,QAAyB,eAAzB;AACA,eAAeN,eAAe,CAAC;AAC7BO,YAAU,EAAE;AACVJ,gBADU;AAEVC;AAFU,GADiB;;AAM7BI,OAAK,GAAG;AACN,UAAMC,SAAQ,GAAIR,QAAQ,CAAC;AACzBS,cAAQ,EAAE,EADe;AAEzBC,cAAQ,EAAE,EAFe;AAGzBC,cAAQ,EAAE;AAHe,KAAD,CAA1B;;AAMA,UAAMC,QAAO,GAAIC,MAAK,IAAK;AACvBR,gBAAU,CACNQ,MAAM,CAACJ,QADD,EACWI,MAAM,CAACH,QADlB,CAAV,CAEEI,IAFF,CAGIC,QAAO,IAAI;AACP;AACAX,cAAM,CAACY,IAAP,CAAY;AAACC,cAAI,EAAC;AAAN,SAAZ;AACH,OANL,EAOIF,QAAO,IAAI,CACP;AACJ,OATJ;AAYH,KAbD;;AAeA,UAAMG,cAAa,GAAIC,SAAQ,IAAK;AAClCC,aAAO,CAACC,GAAR,CAAY,SAAZ,EAAuBF,SAAvB;AACD,KAFD;;AAIA,UAAMG,QAAO,GAAIrB,QAAQ,CAAC,MAAM;AAC9B,aAAO,EAAEO,SAAS,CAACC,QAAV,IAAsBD,SAAS,CAACE,QAAlC,CAAP;AACD,KAFwB,CAAzB;AAGA,WAAO;AACLF,eADK;AAELI,cAFK;AAGLM,oBAHK;AAILI;AAJK,KAAP;AAMD;;AAzC4B,CAAD,CAA9B","names":["defineComponent","reactive","computed","UserOutlined","LockOutlined","router","postSubmit","components","setup","formState","username","password","remember","onFinish","values","then","response","push","path","onFinishFailed","errorInfo","console","log","disabled"],"sourceRoot":"","sources":["/Users/lyong/Desktop/myproj/appfront/src/views/login.vue"],"sourcesContent":["<template>\n  <a-form\n    :model=\"formState\"\n    name=\"normal_login\"\n    class=\"login-form\"\n    @finish=\"onFinish\"\n    @finishFailed=\"onFinishFailed\"\n  >\n  <br>\n  <br>\n  <br>\n  <h1>欢迎登录 </h1>\n    <a-form-item\n      label=\"Username\"\n      name=\"username\"\n      :rules=\"[{ required: true, message: 'Please input your username!' }]\"\n    >\n      <a-input v-model:value=\"formState.username\">\n        <template #prefix>\n          <UserOutlined class=\"site-form-item-icon\" />\n        </template>\n      </a-input>\n    </a-form-item>\n\n    <a-form-item\n      label=\"Password\"\n      name=\"password\"\n      :rules=\"[{ required: true, message: 'Please input your password!' }]\"\n    >\n      <a-input-password v-model:value=\"formState.password\">\n        <template #prefix>\n          <LockOutlined class=\"site-form-item-icon\" />\n        </template>\n      </a-input-password>\n    </a-form-item>\n\n    <div class=\"login-form-wrap\">\n      <a-form-item name=\"remember\" no-style>\n        <a-checkbox v-model:checked=\"formState.remember\">Remember me</a-checkbox>\n      </a-form-item>\n      <a class=\"login-form-forgot\" href=\"\">Forgot password</a>\n    </div>\n\n    <a-form-item>\n      <a-button :disabled=\"disabled\" type=\"primary\" html-type=\"submit\" class=\"login-form-button\">\n        Log in\n      </a-button>\n    </a-form-item>\n  </a-form>\n</template>\n<script>\nimport { defineComponent, reactive, computed } from 'vue';\nimport { UserOutlined, LockOutlined } from '@ant-design/icons-vue';\nimport router from '@/router';\nimport {postSubmit} from '../api/api.js'\nexport default defineComponent({\n  components: {\n    UserOutlined,\n    LockOutlined,\n  },\n\n  setup() {\n    const formState = reactive({\n      username: '',\n      password: '',\n      remember: true,\n    });\n\n    const onFinish = values => {\n        postSubmit(\n            values.username, values.password\n        ).then(\n            response =>{\n                // this.message = ''\n                router.push({path:'/'})\n            },\n            response =>{\n                // this.message = 'username or passwords error!'\n            }\n\n        )\n    };\n\n    const onFinishFailed = errorInfo => {\n      console.log('Failed:', errorInfo);\n    };\n\n    const disabled = computed(() => {\n      return !(formState.username && formState.password);\n    });\n    return {\n      formState,\n      onFinish,\n      onFinishFailed,\n      disabled,\n    };\n  },\n\n});\n</script>\n<style>\n#components-form-demo-normal-login .login-form {\n  max-width: 300px;\n}\n#components-form-demo-normal-login .login-form-wrap {\n  display: flex;\n  align-items: center;\n  justify-content: space-between;\n}\n#components-form-demo-normal-login .login-form-forgot {\n  margin-bottom: 24px;\n}\n#components-form-demo-normal-login .login-form-button {\n  width: 100%;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}