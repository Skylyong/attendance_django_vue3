{"ast":null,"code":"import { defineComponent, reactive, computed } from 'vue';\nimport { UserOutlined, LockOutlined } from '@ant-design/icons-vue'; // import router from '@/router';\n\nimport { useRouter } from 'vue-router';\nimport { postSubmit } from '../api/api.js';\nexport default defineComponent({\n  components: {\n    UserOutlined,\n    LockOutlined\n  },\n\n  setup() {\n    let router = useRouter();\n    const formState = reactive({\n      username: '',\n      password: '',\n      remember: true\n    });\n\n    const onFinish = values => {\n      postSubmit(values.username, values.password).then(response => {\n        // this.message = ''\n        console.log('bb');\n        router.push({\n          path: '/worker'\n        });\n      }, response => {\n        // this.message = 'username or passwords error!'\n        console.log('aaa');\n        router.push({\n          path: '/'\n        });\n      });\n    };\n\n    const onFinishFailed = errorInfo => {\n      console.log('Failed:', errorInfo);\n    };\n\n    const disabled = computed(() => {\n      return !(formState.username && formState.password);\n    });\n    return {\n      formState,\n      onFinish,\n      onFinishFailed,\n      disabled\n    };\n  }\n\n});\n/**\n * \n * 1. 设置最小高度\n * 2. header 美化\n * 3. 左边导航栏美化\n * 4. 表格宽度美化\n * 5. 登录页面\n */","map":{"version":3,"mappings":"AAoDA,SAASA,eAAT,EAA0BC,QAA1B,EAAoCC,QAApC,QAAoD,KAApD;AACA,SAASC,YAAT,EAAuBC,YAAvB,QAA2C,uBAA3C,C,CACA;;AACA,SAAQC,SAAR,QAAwB,YAAxB;AACA,SAAQC,UAAR,QAAyB,eAAzB;AACA,eAAeN,eAAe,CAAC;AAC7BO,YAAU,EAAE;AACVJ,gBADU;AAEVC;AAFU,GADiB;;AAM7BI,OAAK,GAAG;AACN,QAAIC,MAAK,GAAKJ,SAAS,EAAvB;AACA,UAAMK,SAAQ,GAAIT,QAAQ,CAAC;AACzBU,cAAQ,EAAE,EADe;AAEzBC,cAAQ,EAAE,EAFe;AAGzBC,cAAQ,EAAE;AAHe,KAAD,CAA1B;;AAMA,UAAMC,QAAO,GAAIC,MAAK,IAAK;AACvBT,gBAAU,CACNS,MAAM,CAACJ,QADD,EACWI,MAAM,CAACH,QADlB,CAAV,CAEEI,IAFF,CAIIC,QAAO,IAAI;AACP;AACCC,eAAO,CAACC,GAAR,CAAY,IAAZ;AACDV,cAAM,CAACW,IAAP,CAAY;AAACC,cAAI,EAAC;AAAN,SAAZ;AACH,OARL,EASIJ,QAAO,IAAI;AACP;AACAC,eAAO,CAACC,GAAR,CAAY,KAAZ;AACAV,cAAM,CAACW,IAAP,CAAY;AAACC,cAAI,EAAC;AAAN,SAAZ;AACJ,OAbJ;AAgBH,KAjBD;;AAmBA,UAAMC,cAAa,GAAIC,SAAQ,IAAK;AAClCL,aAAO,CAACC,GAAR,CAAY,SAAZ,EAAuBI,SAAvB;AACD,KAFD;;AAIA,UAAMC,QAAO,GAAItB,QAAQ,CAAC,MAAM;AAC9B,aAAO,EAAEQ,SAAS,CAACC,QAAV,IAAsBD,SAAS,CAACE,QAAlC,CAAP;AACD,KAFwB,CAAzB;AAGA,WAAO;AACLF,eADK;AAELI,cAFK;AAGLQ,oBAHK;AAILE;AAJK,KAAP;AAMD;;AA9C4B,CAAD,CAA9B;AAmDA","names":["defineComponent","reactive","computed","UserOutlined","LockOutlined","useRouter","postSubmit","components","setup","router","formState","username","password","remember","onFinish","values","then","response","console","log","push","path","onFinishFailed","errorInfo","disabled"],"sourceRoot":"","sources":["/Users/lyong/Desktop/myproj/appfront/src/views/login.vue"],"sourcesContent":["<template>\n  <a-form\n    :model=\"formState\"\n    name=\"normal_login\"\n    class=\"login-form\"\n    @finish=\"onFinish\"\n    @finishFailed=\"onFinishFailed\"\n  >\n  <br>\n  <br>\n  <br>\n  <h1>欢迎登录 </h1>\n    <a-form-item\n      label=\"Username\"\n      name=\"username\"\n      :rules=\"[{ required: true, message: 'Please input your username!' }]\"\n    >\n      <a-input v-model:value=\"formState.username\">\n        <template #prefix>\n          <UserOutlined class=\"site-form-item-icon\" />\n        </template>\n      </a-input>\n    </a-form-item>\n\n    <a-form-item\n      label=\"Password\"\n      name=\"password\"\n      :rules=\"[{ required: true, message: 'Please input your password!' }]\"\n    >\n      <a-input-password v-model:value=\"formState.password\">\n        <template #prefix>\n          <LockOutlined class=\"site-form-item-icon\" />\n        </template>\n      </a-input-password>\n    </a-form-item>\n\n    <div class=\"login-form-wrap\">\n      <a-form-item name=\"remember\" no-style>\n        <a-checkbox v-model:checked=\"formState.remember\">Remember me</a-checkbox>\n      </a-form-item>\n      <a class=\"login-form-forgot\" href=\"\">Forgot password</a>\n    </div>\n\n    <a-form-item>\n      <a-button :disabled=\"disabled\" type=\"primary\" html-type=\"submit\" class=\"login-form-button\">\n        Log in\n      </a-button>\n      \n    </a-form-item>\n  </a-form>\n</template>\n<script>\nimport { defineComponent, reactive, computed } from 'vue';\nimport { UserOutlined, LockOutlined } from '@ant-design/icons-vue';\n// import router from '@/router';\nimport {useRouter} from 'vue-router'\nimport {postSubmit} from '../api/api.js'\nexport default defineComponent({\n  components: {\n    UserOutlined,\n    LockOutlined,\n  },\n\n  setup() {\n    let router  = useRouter()\n    const formState = reactive({\n      username: '',\n      password: '',\n      remember: true,\n    });\n\n    const onFinish = values => {\n        postSubmit(\n            values.username, values.password\n        ).then(\n          \n            response =>{\n                // this.message = ''\n                 console.log('bb')\n                router.push({path:'/worker'})\n            },\n            response =>{\n                // this.message = 'username or passwords error!'\n                console.log('aaa')\n                router.push({path:'/'})\n            }\n\n        )\n    };\n\n    const onFinishFailed = errorInfo => {\n      console.log('Failed:', errorInfo);\n    };\n\n    const disabled = computed(() => {\n      return !(formState.username && formState.password);\n    });\n    return {\n      formState,\n      onFinish,\n      onFinishFailed,\n      disabled,\n    };\n  },\n\n});\n\n\n/**\n * \n * 1. 设置最小高度\n * 2. header 美化\n * 3. 左边导航栏美化\n * 4. 表格宽度美化\n * 5. 登录页面\n */\n</script>\n<style>\n#components-form-demo-normal-login .login-form {\n  max-width: 300px;\n}\n#components-form-demo-normal-login .login-form-wrap {\n  display: flex;\n  align-items: center;\n  justify-content: space-between;\n}\n#components-form-demo-normal-login .login-form-forgot {\n  margin-bottom: 24px;\n}\n#components-form-demo-normal-login .login-form-button {\n  width: 100%;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}