a);\n    };\n\n    return {\n      options,\n      columns,\n      onDelete,\n      handleAdd,\n      dataSource,\n      editableData,\n      count,\n      edit,\n      save\n    };\n  }\n\n});","map":{"version":3,"mappings":"AAkEA,SAASA,QAAT,EAAmBC,eAAnB,EAAoCC,QAApC,EAA8CC,GAA9C,QAAyD,KAAzD;AACA,SAASC,aAAT,EAAwBC,YAAxB,QAA4C,uBAA5C;AACA,SAASC,SAAT,QAA0B,WAA1B;AACA,SAASC,OAAT,QAAwB,gBAAxB;AACA,eAAeN,eAAe,CAAC;AAC7BO,YAAU,EAAE;AACVJ,iBADU;AAEVC;AAFU,GADiB;;AAM7BI,OAAK,GAAG;AACN,UAAMC,OAAM,GAAIP,GAAG,CAAC,CAClB;AACEQ,WAAK,EAAE,GADT;AAEEC,WAAK,EAAE;AAFT,KADkB,EAKlB;AACED,WAAK,EAAE,GADT;AAEEC,WAAK,EAAE;AAFT,KALkB,CAAD,CAAnB;AAWA,UAAMC,OAAM,GAAI,CACd;AACEC,WAAK,EAAE,MADT;AAEEC,eAAS,EAAE,MAFb;AAGEC,WAAK,EAAE,KAHT;AAIEC,WAAK,EAAE;AACLC,oBAAY,EAAE;AADT;AAJT,KADc,EASd;AACEJ,WAAK,EAAE,KADT;AAEEC,eAAS,EAAE,KAFb;AAGEE,WAAK,EAAE;AACLC,oBAAY,EAAE;AADT;AAHT,KATc,EAgBd;AACEJ,WAAK,EAAE,SADT;AAEEC,eAAS,EAAE;AAFb,KAhBc,EAoBd;AACED,WAAK,EAAE,WADT;AAEEC,eAAS,EAAE,WAFb;AAGEE,WAAK,EAAE;AACLC,oBAAY,EAAE;AADT;AAHT,KApBc,CAAhB;AA4BA,UAAMC,UAAS,GAAIhB,GAAG,CAAC,CACrB;AACEiB,SAAG,EAAE,GADP;AAEEC,UAAI,EAAE,eAFR;AAGEC,SAAG,EAAE,EAHP;AAIEC,aAAO,EAAE;AAJX,KADqB,EAOrB;AACEH,SAAG,EAAE,GADP;AAEEC,UAAI,EAAE,eAFR;AAGEC,SAAG,EAAE,EAHP;AAIEC,aAAO,EAAE;AAJX,KAPqB,CAAD,CAAtB;AAcA,UAAMC,KAAI,GAAIxB,QAAQ,CAAC,MAAMmB,UAAU,CAACR,KAAX,CAAiBc,MAAjB,GAA0B,CAAjC,CAAtB;AACA,UAAMC,YAAW,GAAIxB,QAAQ,CAAC,EAAD,CAA7B;;AAEA,UAAMyB,IAAG,GAAIP,GAAE,IAAK;AAClBM,kBAAY,CAACN,GAAD,CAAZ,GAAoBd,SAAS,CAACa,UAAU,CAACR,KAAX,CAAiBiB,MAAjB,CAAwBC,IAAG,IAAKT,GAAE,KAAMS,IAAI,CAACT,GAA7C,EAAkD,CAAlD,CAAD,CAA7B;AACAb,aAAO,CAACuB,OAAR,CAAgBJ,YAAhB;AACD,KAHD;;AAKA,UAAMK,IAAG,GAAIX,GAAE,IAAK;AAClBY,YAAM,CAACC,MAAP,CAAcd,UAAU,CAACR,KAAX,CAAiBiB,MAAjB,CAAwBC,IAAG,IAAKT,GAAE,KAAMS,IAAI,CAACT,GAA7C,EAAkD,CAAlD,CAAd,EAAoEM,YAAY,CAACN,GAAD,CAAhF;AACA,aAAOM,YAAY,CAACN,GAAD,CAAnB;AACD,KAHD;;AAMA,UAAMc,QAAO,GAAId,GAAE,IAAK;AACtBD,gBAAU,CAACR,KAAX,GAAmBQ,UAAU,CAACR,KAAX,CAAiBiB,MAAjB,CAAwBC,IAAG,IAAKA,IAAI,CAACT,GAAL,KAAaA,GAA7C,CAAnB;AACD,KAFD;;AAI