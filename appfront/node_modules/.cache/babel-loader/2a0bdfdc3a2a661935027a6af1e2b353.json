{"ast":null,"code":"import { getBooks, postBook } from '../api/api.js';\nexport default {\n  name: 'HelloWorld',\n\n  data() {\n    return {\n      msg: 'Welcome to Your Vue.js App',\n      // books list data\n      books: [{\n        name: 'test',\n        author: 't'\n      }, {\n        name: 'test2',\n        author: 't2'\n      }],\n      // book data in the form\n      inputBook: {\n        \"name\": \"\",\n        \"author\": \"\"\n      }\n    };\n  },\n\n  methods: {\n    loadBooks() {\n      getBooks().then(response => {\n        this.books = response.data;\n      });\n    },\n\n    // load books list when visit the page\n    bookSubmit() {\n      postBook(this.inputBook);\n    } // add a book to backend when click the button\n\n\n  },\n  created: function () {\n    this.loadBooks();\n  }\n};","map":{"version":3,"mappings":"AAmBA,SAAQA,QAAR,EAAkBC,QAAlB,QAAiC,eAAjC;AACA,eAAe;AACbC,MAAI,EAAE,YADO;;AAEbC,MAAG,GAAK;AACN,WAAO;AACLC,SAAG,EAAE,4BADA;AAEL;AACAC,WAAK,EAAE,CACL;AAACH,YAAI,EAAE,MAAP;AAAeI,cAAM,EAAE;AAAvB,OADK,EAEL;AAACJ,YAAI,EAAE,OAAP;AAAgBI,cAAM,EAAE;AAAxB,OAFK,CAHF;AAOL;AACAC,eAAS,EAAE;AACT,gBAAQ,EADC;AAET,kBAAU;AAFD;AARN,KAAP;AAaD,GAhBY;;AAiBbC,SAAO,EAAE;AACPC,aAAQ,GAAK;AACXT,cAAQ,GAAGU,IAAX,CACEC,QAAQ,IAAE;AACR,aAAKN,KAAL,GAAaM,QAAQ,CAACR,IAAtB;AACF,OAHF;AAKD,KAPM;;AAOJ;AACHS,cAAS,GAAK;AACZX,cAAQ,CACN,KAAKM,SADC,CAAR;AAGF,KAZO,CAYL;;;AAZK,GAjBI;AA+BbM,SAAO,EAAE,YAAY;AACnB,SAAKJ,SAAL;AACF;AAjCa,CAAf","names":["getBooks","postBook","name","data","msg","books","author","inputBook","methods","loadBooks","then","response","bookSubmit","created"],"sourceRoot":"","sources":["/Users/lyong/Desktop/myproj/appfront/src/components/HelloWorld.vue"],"sourcesContent":["<template>\n  <div class=\"hello\">\n    <h1>{{ msg }}</h1>\n    <!-- show books list -->\n    <ul>\n      <li v-for=\"(book, index) in books\" :key=\"index\" style=\"display:block\">\n        {{index}}-{{book.name}}-{{book.author}}\n      </li>\n    </ul>\n    <!-- form to add a book -->\n    <form action=\"\">\n      输入书名：<input type=\"text\" placeholder=\"book name\" v-model=\"inputBook.name\"><br>\n      输入作者：<input type=\"text\" placeholder=\"book author\" v-model=\"inputBook.author\"><br>\n    </form>\n    <button type=\"submit\" @click=\"bookSubmit()\">submit</button>\n  </div>\n</template>\n\n<script>\nimport {getBooks, postBook} from '../api/api.js'\nexport default {\n  name: 'HelloWorld',\n  data () {\n    return {\n      msg: 'Welcome to Your Vue.js App',\n      // books list data\n      books: [\n        {name: 'test', author: 't'},\n        {name: 'test2', author: 't2'}\n      ],\n      // book data in the form\n      inputBook: {\n        \"name\": \"\",\n        \"author\": \"\",\n      }\n    }\n  },\n  methods: {\n    loadBooks () {\n      getBooks().then(\n        response=>{\n          this.books = response.data\n        }\n      )\n    }, // load books list when visit the page\n    bookSubmit () {\n      postBook(\n        this.inputBook\n      )\n    } // add a book to backend when click the button\n  },\n  created: function () {\n    this.loadBooks()\n  }\n}\n</script>"]},"metadata":{},"sourceType":"module"}