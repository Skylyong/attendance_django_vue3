{
    "version": "0.2.0",
    "configurations": [
    

      {
        "type": "chrome",
        "request": "launch",
        "name": "vuejs: chrome",
        "url": "http://127.0.0.1:8080",
        "webRoot": "${workspaceFolder}/src",
        "breakOnLoad": true,
        "sourceMapPathOverrides": {
          "webpack:///src/*": "${webRoot}/*"
        }
      },
    ]
  }